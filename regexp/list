\s 0x20, 0x80
\b
place = "japan"
/#{palce}/.match("Go to Japan") # => <Matchdata "Japan">
number = "(\\d+)"
operator = "(\\+|-|\\*|/)"
/#{number}#{operator}#{number}/.match("43+291") # => #<MatchData "43+291" 1:"43" 2:"+" 3:"291">
