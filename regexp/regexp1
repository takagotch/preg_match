^
(?:
  ISBN
  (?:-1[03])?   #-10|-13, ?=a|b
  :?            #?=\ |: 
  \
)?
(?=
  [-0-9\ ]{17}$
 |
  [0-9X\ ]{13}$
)
(?:
  97[89]       #978|979
  [-\ ]?
)?
[0-9]{1,5}
[-\ ]?
(?:
  [0-9]+
  [-\ ]?
){2}
[0-9X]
$

